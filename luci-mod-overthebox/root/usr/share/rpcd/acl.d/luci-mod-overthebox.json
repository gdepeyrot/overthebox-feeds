{
  "luci-mod-overthebox-overview": {
    "description": "Grant UCI access to OverTheBox configuration",
    "read": {
      "file": {
        "/etc/otb-version": [
          "read"
        ],
        "/tmp/otb-data/*/*": [
          "read"
        ]
      },
      "ubus": {
        "file": [
          "read"
        ],
        "system": [
          "board",
          "info"
        ],
        "uci": [
          "get"
        ]
      },
      "uci": [
        "overthebox", "dscp"
      ]
    },
    "write": {
      "ubus": {
        "uci": [
          "set",
          "commit"
        ]
      },
      "file": {
          "/bin/otb-dscp-reset" : [ "exec" ],
          "/usr/bin/awk" : [ "exec" ]
      },
      "uci": [
        "overthebox", "dscp"
      ]
    }
  }
}
